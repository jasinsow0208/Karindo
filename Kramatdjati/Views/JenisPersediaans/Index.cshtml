@model IEnumerable<Kramatdjati.Models.JenisPersediaan>

@{
    ViewBag.Title = "Jenis Persediaan";
}

<h2>Jenis Persediaan</h2>

<p>
    @Html.ActionLink("Tambah", "Create", "JenisPersediaans", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Keterangan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Keterangan)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", "JenisPersediaans", new { id=item.JenisPersediaanID }, new { @class = "btn btn-xs btn-primary" })
            @Html.ActionLink("Hapus", "Delete", "JenisPersediaans", new { id = item.JenisPersediaanID }, new { @class = "btn btn-xs btn-danger" })
        </td>
    </tr>
}

</table>
