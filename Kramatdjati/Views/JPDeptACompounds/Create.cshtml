@model Kramatdjati.Models.JPDeptACompound

@{
    ViewBag.Title = "Create";
}

<h2>Tambah</h2>
<h4>Jadual Produksi Departemen A</h4>
<h4>Perincian Pemakaian Compound</h4>
<hr />
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
          
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.JPDeptAID, new { Value = ViewBag.JPDeptAID })
      
