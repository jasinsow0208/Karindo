@model Kramatdjati.Models.GudangBahanBaku

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
<h4>Persediaan @ViewBag.Gudang</h4>
<hr />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
  
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.GudangBahanBakuID)
        @Html.HiddenFor(model => model.GudangID)
        @Html.HiddenFor(model => model.Jumlah)

          <div class="form-group">
            @Html.LabelFor(model => model.BahanBakuID, "BahanBakuID", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("BahanBakuID", null, htmlAttributes: new { @class = "form-control" })
                <label id="NamaBarang" class="control-label "></label>
                @Html.ValidationMessageFor(model => model.BahanBakuID, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index", new { id = @ViewBag.GudangID })
</div>
